  "license": "ISC",
  "devDependencies": {
    "@types/node": "^22.9.0",
    "@types/pdfmake": "^0.2.9",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "pdfmake": "^0.2.15"
  }
}
